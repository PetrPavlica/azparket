{
	"name": "nette/sandbox",
	"description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
	"homepage": "https://nette.org",
	"type": "project",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "https://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "https://nette.org/contributors"
		}
	],
	"require": {
		"php": ">=7.4",
		"nette/application": "^3.1",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.1",
		"nette/database": "^3.1",
		"nette/di": "3.1.6",
		"nette/finder": "^2.5",
		"nette/forms": "^3.1",
		"nette/http": "^3.1",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.3",
		"nette/safe-stream": "^2.4",
		"nette/security": "^3.1",
		"nette/utils": "^3.2",
		"latte/latte": "^2.9",
		"tracy/tracy": "^2.8",
		"nettrine/annotations": "^0.7.0",
		"nettrine/cache": "^0.3.0",
		"nettrine/migrations": "^0.7.0",
		"nettrine/fixtures": "^0.6.0",
		"nettrine/dbal": "^0.7.0",
		"nettrine/orm": "^0.8.1",
		"monolog/monolog": "^2.2",
		"contributte/console": "^0.9.0",
		"contributte/console-extra": "^0.6.0",
		"contributte/monolog": "^0.5.0",
		"ublaboo/datagrid": "^6.8",
		"contributte/translation": "^1.1",
		"nettrine/extensions-beberlei": "^0.2.0",
		"mpdf/mpdf": "^8.0",
		"phpoffice/phpspreadsheet": "^1.17",
		"dfridrich/ares": "^2.0",
		"contributte/recaptcha": "^3.4",
		"contributte/apitte": "^0.10.2",
		"symfony/serializer": "^5.4",
		"symfony/validator": "^5.4",
		"symfony/property-info": "^5.4",
		"contributte/di": "^0.5.3"
	},
	"require-dev": {
		"dg/adminer-custom": "^1.25",
		"nette/tester": "^2.3",
		"phpstan/phpstan-nette": "^0.12",
		"symfony/thanks": "^1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse"
	},
	"config": {
		"allow-plugins": {
			"symfony/thanks": false
		}
	}
}
